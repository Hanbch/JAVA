package chap05;

public class Method01 {
	
	public static void hiEveryone(int age, double height) {//1. 인자 전달을 하지만 리턴 값이 없는 함수 생성
		System.out.println("제 나이는" + age + "세입니다." );
		System.out.println("제 키는" + height + "cm입니다." );
	}
	
	public static void byeEveryone() {//2. 인자 전달을 하지않고 리턴값이 없는 함수 생성
		System.out.println("bye");
	}
	
	public static int adder(int num1, int num2) {//3.인자전달을 하고 리턴값을 받는 함수 생성
		int addRst = num1 + num2;
		return addRst;
	}
	
	public static double square(double num) {//3.인자전달을 하고 리턴값을 받는 함수 생성
		return num * num;

	}
	

	public static void main(String[] args) {
		double myHeight = 175.0;
		//1. 인자 전달을 하지만 리턴 값이 없는 함수 호출
		  hiEveryone(12, 12.5);
		  hiEveryone(13, myHeight);
		  
		//2. 인자 전달을 하지않고 리턴값이 없는 함수 호출
		byeEveryone();
		
		//3.인자전달을 하고 리턴값을 받는 함수 호출
		int rst = adder(4,5);
		int rst2 = square(3.5);
		System.out.println(rst);
		
	}

}
