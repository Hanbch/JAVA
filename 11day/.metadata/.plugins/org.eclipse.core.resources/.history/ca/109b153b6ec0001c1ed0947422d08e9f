package chap08;



public class Review {
	
	public static int adding(int num) {
		int sum = 0;
		for (int i=1; i<=num; i++) {
			sum += i;
		}
		return sum;
		
	}
	
	public static void main(String[] args) {
		// 하나의 정수를  받아서 해당 1 부터 해당정수 까지의 합을 리턴 하는 함수
		int rst = adding(10);
		System.out.println(rst);
	}

}
